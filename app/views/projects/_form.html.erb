<%= @project.errors.full_messages.join(". ") %>

<%= form_for [current_user, @project] do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>
  <div>
    <%= f.label :url %>
    <%= f.url_field :url %>
  </div>
  <div>
    <%= f.label :github %>
    <%= f.url_field :github %>
  </div>
  <div>
    <%= f.file_field :image %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>
